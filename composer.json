        "php": "^7.2",
        "laravel/framework": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "laravel/laravel": "~5.5|~6.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
